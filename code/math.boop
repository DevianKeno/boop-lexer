    public static func Max(float a, float b) -> float
    {
        if (a > b)
        {
            return a;
        } else if (a < b)
        {
            return b;
        } else
        {
            return 0;
        }
    }
    
    public static func Min(float a, float b) -> float
    {
        if (a < b)
        {
            return a;
        } else if (a > b)
        {
            return b;
        } else
        {
            return 0;
        }
    }

    public static func Abs(int x) -> float
    {
        if (x < 0) { return x - x; }
        else { return x; }
    }

