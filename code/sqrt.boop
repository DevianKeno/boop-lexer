class SquareRoots
{
    func Main()
    {
        # simple program of getting square root of a number
        # declaration of variables
        public float num;
        public float sqrt;

        # getting an user input of user
        string input = ReadLine(“Enter A Number: ”);
        num = ToFloat(input);
        sqrt = √num;

        # printing of square rooted number
        Print(sqrt);
    }	
}
